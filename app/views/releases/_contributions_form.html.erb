<div id="release-contributions">
    <strong>Artist Contributions:</strong>
    <ul class="nested_form contributions">
        <%= f.fields_for :contributions do |ff| %>
            <%= render 'contribution_fields', f: ff %>
        <% end %>
        <li>
        <div class="links">
            <%= link_to_add_association 'Add Contribution', f, :contributions, class: 'btn btn-success' %>
        </div>
        </li>
    </ul>
</div>
